{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ROBOTS": "NOINDEX, NOFOLLOW",
      "ms.author": "duncanma",
      "ms.service": "non-product-specific",
      "ms.date": "03/13/2017",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/SamplesPages",
      "feedback_product_url": "mailto://duncanma@microsoft.com"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "pages",
    "markdownEngineName": "markdig"
  }
}
